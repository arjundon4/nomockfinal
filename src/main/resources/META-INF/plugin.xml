<idea-plugin>
    <id>com.sf.nomockfinal</id>
    <name>No Mock Stub Generator</name>
    <vendor email="arjun.vinod@sap.com" url="https://github.com/arjundon4/nomockfinal">No Mock Stub Generator</vendor>

    <description><![CDATA[Plugin to generator Stub class for No Mock Framework]]></description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>

    <actions>
        <action
                id="SF.NOMockFramework.Editor"
                class="com.sap.sf.nomockfw.MainAction"
                text="Create NoMock Boilerplate Code"
                description="Create boilerplate code with custom implementation for No Mock Framework">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
        </action>
    </actions>
</idea-plugin>